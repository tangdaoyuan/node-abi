{
  "name": "node-abi",
  "version": "0.0.0-development",
  "description": "Get the Node ABI for a given target and runtime, and vice versa.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/index.js",
    "update-abi-registry": "node --unhandled-rejections=strict scripts/update-abi-registry.js",
    "release": "bumpp --tag --commit --push && pnpm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgeiger/node-abi.git"
  },
  "keywords": [
    "node",
    "electron",
    "node_module_version",
    "abi",
    "v8"
  ],
  "author": "Lukas Geiger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgeiger/node-abi/issues"
  },
  "homepage": "https://github.com/lgeiger/node-abi#readme",
  "bin": {
    "node-abi": "./bin/node-abi.js"
  },
  "files": [
    "bin",
    "index.js",
    "cli.js"
  ],
  "devDependencies": {
    "@types/yargs": "^17.0.10",
    "bumpp": "^8.2.1",
    "got": "^11.8.2",
    "tape": "^5.3.1"
  },
  "dependencies": {
    "picocolors": "^1.0.0",
    "semver": "^7.3.5",
    "yargs": "^17.5.1"
  },
  "engines": {
    "node": ">=10"
  }
}
